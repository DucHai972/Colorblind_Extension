{
  "manifest_version": 3,
  "name": "Deep Learning Image Predictor",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "http://127.0.0.1:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
